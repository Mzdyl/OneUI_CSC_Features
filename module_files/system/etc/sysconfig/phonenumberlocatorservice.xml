<?xml version="1.0" encoding="UTF-8"?>
<config>
    <!-- These are the standard packages that are white-listed to always have internet access while in power save mode, even if they aren't in the foreground. -->
	<!-- phonenumberlocatorservice must always have network access for provide service to call app and contact app. -->
	
    <allow-in-power-save package="com.sgmc.phonenumberlocatorservice" />
    <allow-in-power-save-except-idle package="com.sgmc.phonenumberlocatorservice" />
    <allow-in-data-usage-save package="com.sgmc.phonenumberlocatorservice" />
</config>